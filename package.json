  "license": "MIT",
  "dependencies": {
    "knex": "^0.15.2",
    "pg": "^7.6.1",
    "wraps-logger": "^1.0.6"
  },
  "devDependencies": {
    "@types/knex": "^0.15.1",
    "@types/pg": "^7.4.11",
    "tslint-config-prettier": "^1.16.0",
    "wraps-base": "0.0.0"
  }
